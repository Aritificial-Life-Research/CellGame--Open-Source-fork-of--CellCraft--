package com.pecLevel{	public class LevelStuff extends Object	{		public var goodies:Vector.<StuffEntry> = new Vector.<StuffEntry>();  		public var enemies:Vector.<StuffEntry> = new Vector.<StuffEntry>(); 		public var objects:Vector.<StuffEntry> = new Vector.<StuffEntry>();			public function toString():String {			var str:String = "LevelStuff{\n";			for each (var g:StuffEntry in goodies) {				str += "     goodie(n=" + g.stuff_name + " t=" + g.stuff_type + " c=" + g.stuff_count +  " s=" + g.stuff_spawn + ")\n";			}			for each (var e:StuffEntry in enemies) {				str += "     enemy(n=" + e.stuff_name + " t=" + e.stuff_type + " c=" + e.stuff_count + " s=" + e.stuff_spawn + ")\n";			}			for each (var o:StuffEntry in objects) {				str += "     object(n=" + o.stuff_name + " t=" + o.stuff_type + " c=" + o.stuff_count + " s=" + o.stuff_spawn + ")\n";			}			str += "}\n";			return str;		}	}}