package com.pecLevel{	public class LevelThings extends Object	{		public var goodies:Vector.<ThingEntry> = new Vector.<ThingEntry>();  		public var waves:Vector.<WaveEntry> = new Vector.<WaveEntry>(); 		public var objects:Vector.<ThingEntry> = new Vector.<ThingEntry>();			public function toString():String {			var str:String = "LevelThings{\n";			for each (var g:ThingEntry in goodies) {				str += "goodie(n=" + g.thing_name + " t=" + g.thing_type + " c=" + g.thing_count +  " s=" + g.thing_spawn + ")\n";			}			for each (var e:WaveEntry in waves) {				str += "wave(n=" + e.id + " t=" + e.type + " c=" + e.count +  " a=" + e.active + ")\n";			}			for each (var o:ThingEntry in objects) {				str += "object(n=" + o.thing_name + " t=" + o.thing_type + " c=" + o.thing_count +  " s=" + o.thing_spawn + ")\n";			}			str += "}\n";			return str;		}		}}