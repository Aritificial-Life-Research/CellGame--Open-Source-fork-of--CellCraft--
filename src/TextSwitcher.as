package{	import flash.display.MovieClip;		public class TextSwitcher extends MovieClip{		public static const frameList:Vector.<String> = Vector.<String>(["root", "pseudopod", "plant", "animal", "membrane", "cytoplasm", "metabolism", "nucleus", "er", "golgi", "centrosome", "mitochondrion", "chloroplast", "ribosome", "slicer", "injector", "invader", "infester", "radical", "peroxisome", "lysosome", "recycling", "atp", "fa", "na", "aa", "g", "protein", "dna", "mrna", "defensin", "brown_fat", "necrosis", "platypus"]);// , "sydney", "spike", "bill", "jeeves", "monotremus", "e4r1h"]);				public function nextEntry():void{			var currIndex:int = frameList.indexOf(this.currentLabel);			gotoAndStop(frameList[(currIndex+1)>(frameList.length-1)?0:(currIndex+1)]);			//trace(this.currentLabel);		}				public function prevEntry():void{			var currIndex:int = frameList.indexOf(this.currentLabel);			gotoAndStop(frameList[(currIndex-1)<0?(frameList.length-1):(currIndex-1)]);			//trace(this.currentLabel);		}				public function getNextEntry():String {			var currIndex:int = frameList.indexOf(this.currentLabel);			currIndex += 1;			if (currIndex > frameList.length - 1) {				currIndex = 0;			}			trace("currIndex = " + currIndex);			return frameList[currIndex];			//return frameList[(currIndex + 1) > (frameList.length - 1)?0:(currIndex + 1)];		}		public function getPrevEntry():String {			var currIndex:int = frameList.indexOf(this.currentLabel);			currIndex -= 1;			if (currIndex < 0) {				currIndex = frameList.length - 1;				}			trace("currIndex = " + currIndex);			return frameList[currIndex];			//return frameList[(currIndex-1)<0?(frameList.length-1):(currIndex-1)]		}	}	}